<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0" xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc"
  xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.0.0/StyledLayerDescriptor.xsd">
  <NamedLayer>
    <Name>rain</Name>
    <UserStyle>
      <Name>rain</Name>
      <Title>Rain distribution</Title>
      <FeatureTypeStyle>
        <Rule>
          <RasterSymbolizer>
            <Opacity>1.0</Opacity>
            <ColorMap type="intervals">
              <ColorMapEntry color="#174C74" opacity="0.8" quantity="1" label="0-Water"/>
              <ColorMapEntry color="#162103" quantity="2" label="1-Evergreen Needleleaf Forest"/>       
              <ColorMapEntry color="#235123" quantity="3" label="2-Evergreen Broadleaf Forest"/>       
              <ColorMapEntry color="#399B38" quantity="4" label="3-Deciduous Needleleaf Forest"/>  
              <ColorMapEntry color="#38EB38" quantity="5" label="4-Deciduous Broadleaf Forest"/>      
              <ColorMapEntry color="#39723B" quantity="6" label="5-Mixed Forests"/>        
              <ColorMapEntry color="#6A2424" quantity="7" label="6-Closed Shrubland"/>    
              <ColorMapEntry color="#C3A55F" quantity="8" label="7-Open Shrublands"/>         
              <ColorMapEntry color="#B76124" quantity="9" label="8-Woody Savannas"/>        
              <ColorMapEntry color="#DD9426" quantity="10" label="9-Savannas"/>        
              <ColorMapEntry color="#92AF1F" quantity="11" label="10-Grasslands"/>      
              <ColorMapEntry color="#10104C" quantity="12" label="11-Permanent Wetlands"/>  
              <ColorMapEntry color="#CDB400" quantity="13" label="12-Croplands"/>  
              <ColorMapEntry color="#CC0202" quantity="14" label="13-Urban and Built-In"/>  
              <ColorMapEntry color="#332808" quantity="15" label="14-Cropland/Natural Vegetation Mosaic"/>  
              <ColorMapEntry color="#D7CDCC" quantity="16" label="15-Snow and Ice"/>  
              <ColorMapEntry color="#F7E174" quantity="17" label="16-Barren or Sparsely Vegetated"/>  
            </ColorMap>
          </RasterSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

